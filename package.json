{
  "name": "Timekeeper",
  "version": "1.0.0",
  "description": "Timekeeper version 2.0",
  "main": "main.js",
  "build": {
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "icon": "/photos/timer.png",
    "nsis": {
      "oneClick": false
    }
  },
  "scripts": {
    "start": "electron .",
    "build-all": "electron-builder -mwl",
    "build-w": "electron-builder -w"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "RZI",
  "license": "MIT",
  "devDependencies": {
    "electron": "^16.0.5",
    "electron-builder": "^22.14.13"
  },
  "dependencies": {
    "chart.js": "^2.9.3"
  }
}
